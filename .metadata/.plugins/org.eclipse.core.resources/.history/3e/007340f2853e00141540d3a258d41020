#include "SimpleDate.h"
#include <stdio.h>

int main() {
	SimpleDate date(9,17,2014);
	printf("Day of Week: %d", date.dayOfWeek());
	printf("Days from now: %d", date.daysFromNow(2));
	return 0;
}
